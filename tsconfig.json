{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "app/src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/index.jsx",
    "app/_layout.jsx",
    "app/(tabs)/explore.jsx",
    "app/(tabs)/_layout.jsx",
    "nativewind-env.d.ts"
, "app/+not-found.jsx", "app/src/hooks/useColorScheme.jsx", "app/src/hooks/useColorScheme.web.jsx", "app/src/hooks/useThemeColor.ts"  ]
}